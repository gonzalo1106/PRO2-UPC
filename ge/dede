using System;

class Program
{
    static void Main()
    {
        // Crea una instancia de la clase Random
        Random random = new Random();

        // Genera un número aleatorio entre 0 y 999999999
        int numeroAleatorio = random.Next(0, 999999999);

        // Muestra el número aleatorio generado
        Console.WriteLine("Número aleatorio generado: " + numeroAleatorio);
    }
}